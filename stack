#include<stdio.h>
void main()
{
 int i=1,n,a[100],b[100];
 int j=0,m=1,k=0,flag=0;
 printf("enter the limit of songs\n");
 scanf("%d",&n);
 printf("enter the order of songs\n");
 for(i=0;i<n;i++)
{
 scanf("\n%d",&a[i]);
}
 i=1;
 while(k!=n)
 {
  if(i!=a[k])
  {
   b[j]=a[k];
   j=j+1;
  }
  else
  {
   i=i+1;
  }
  k=k+1;
 }
 if(j>0)
 {
    for(i=j-1;i>=0;i++)
  {
    if(b[i]>b[i-1])
    {
      flag=1;
      break;
    }
  }
 }
if(flag==1)
{
 printf("sad");
}
else
{
printf("happy");
}
}
